/*profit % of each month 
in the year 2019*/
SELECT t1.months, SUM(t1.profit_percent) profit_percent, t1.years
FROM
	(SELECT months, (100.0*profit/SUM(profit)OVER())  profit_percent, years
	FROM int_brew 
	WHERE years ='2019' AND months BETWEEN 'January' AND 'November'
	GROUP BY 1, profit, 3
	ORDER BY 1) t1
GROUP BY 1,3
ORDER BY 2 DESC;
